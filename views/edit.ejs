<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <!-- Bootstrap CSS -->
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css"
  integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
  <link rel='stylesheet' href='/stylesheets/style.css' />
  <title>Phone edit</title>
</head>
<body>
  <main>
    <div class="bloque1 ">
      <fieldset>
        <legend>Editar Usuario</legend>
        <% result.forEach( function(user){ %>
        <form class="formulario" action="/users/update/<%- user.user_id %>" method="post">
          <hr>  
          <div class="date_user my-4">
            <label class="w-100" for="name">Name</label>
            <input type="text" name="name" value="<%- user.name %>">
          </div>
          <div class="date_user my-4">
            <label class="w-100" for="last_name">Last Name</label>
            <input type="text" name="last_name" value=" <%- user.last_name %>">
          </div>
          <div class="date_user my-4">
            <label class="w-100" for="model">Model</label>
            <input type="text" name="model" value=" <%- user.model %>">
          </div>
          <div class="date_user my-4">
            <label class="w-100 " for="color">Color</label>
            <input type="text" name="color" value=" <%- user.color %>">
          </div>
          <button type="submit" class="btn button1 btn-primary my-2">Editar</button>
        </form>
        <% }); %> 
      </fieldset>
    </div>      
  </main>


 

  <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
  <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js" integrity="sha384-OgVRvuATP1z7JjHLkuOU7Xw704+h835Lr+6QL9UvYjZE3Ipu6Tp75j7Bh/kR0JKI" crossorigin="anonymous"></script>
</body>
</html>